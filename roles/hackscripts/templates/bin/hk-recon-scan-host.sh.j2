#!/bin/bash

#set -x

# input: in/IPs.txt
# output: console

. {{ hackscripts_base_path }}/lib/hacklib.sh

while read -r line; do
    TargetIP=$( str_strip $line )

    print_title $TargetIP

    cmd=$(get_cmd recon_host)

    cmd=$( replace_variable "$cmd" TargetIP ${TargetIP} )

    exec_cmd "$cmd"
done < ./in/hosts.txt
